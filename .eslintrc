{
  "root": true,
  "extends": ["eslint:recommended", "prettier"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "2021",
    "sourceType": "module"
  },
  "globals": {
    "grecaptcha": "readonly",
    "MktoForms2": "readonly",
    "YT": "readonly"
  },
  "rules": {
    "no-unused-vars": ["error", { "varsIgnorePattern": "slider" }]
  }
}
