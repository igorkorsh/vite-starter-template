{
  "name": "vite-starter-template",
  "private": true,
  "version": "1.0.5",
  "type": "module",
  "projectId": "000000",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "deploy:marketo": "vite build --mode marketo",
    "lint:html": "pnpm exec linthtml src/*.html",
    "lint:styles": "stylelint \"src/styles/**/*.scss\" --custom-syntax postcss-scss",
    "lint:scripts": "eslint . --ext .js",
    "lint:all": "pnpm run lint:html && pnpm run lint:styles && pnpm run lint:scripts",
    "format:styles": "stylelint --fix \"src/styles/**/*.scss\" --custom-syntax postcss-scss",
    "format:scripts": "prettier . --write",
    "format:all": "pnpm run format:styles && pnpm run format:scripts"
  },
  "devDependencies": {
    "@linthtml/linthtml": "^0.9.5",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "fast-glob": "^3.3.2",
    "lightningcss": "^1.22.1",
    "node-html-parser": "^6.1.11",
    "postcss": "^8.4.32",
    "postcss-inline-svg": "^6.0.0",
    "postcss-rtl": "^2.0.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.0",
    "sass": "^1.69.5",
    "sharp": "^0.33.0",
    "stylelint": "^15.11.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-prettier": "^4.1.0",
    "svgo": "^3.0.5",
    "vite": "^5.0.7",
    "vite-plugin-image-optimizer": "^1.1.7"
  },
  "dependencies": {
    "sal.js": "^0.8.5",
    "swiper": "^11.0.5"
  }
}
